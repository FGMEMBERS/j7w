<PropertyList>
<path>magcompass.ac</path>

  <animation>
    <type>rotate</type>
    <object-name>face</object-name>
    <property>/instrumentation/magnetic-compass/indicated-heading-deg</property>
    <center>
     <x-m>-0.0181</x-m>
     <y-m>0</y-m>
     <z-m>0</z-m>
    </center>
    <axis>
     <x>0</x>
     <y>0</y>
     <z>1</z>
    </axis>
  </animation>
</PropertyList>

