<?xml version="1.0"?>
<PropertyList>

 <path>boost.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>Boost.needle</object-name>
  <property>engines/engine/boost-gauge-mmhg</property>
  <interpolation>
     <entry><ind>-800</ind><dep>-180</dep></entry> <!-- -800mmHg  -->  
     <entry><ind>0</ind><dep>0</dep></entry>
     <entry><ind>800</ind><dep>180</dep></entry> <!-- +800mmHg-->
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>
