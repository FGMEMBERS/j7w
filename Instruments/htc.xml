<?xml version="1.0"?>
<PropertyList>
<path>htc.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>HTC.needle</object-name>
  <property>/instrumentation/htc/pressure-norm</property>
  <interpolation>
    <entry><ind>0.0</ind><dep>0.0</dep></entry>
    <entry><ind>1.0</ind><dep>-240.0</dep></entry>
  </interpolation>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
</PropertyList>
